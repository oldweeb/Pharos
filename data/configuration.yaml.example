accounts:
  - private_key: x
    proxy: x
    auth_key: x

settings:
  accounts_mode: sequential
  randomize_feature_order: false
  swaps:
    percentage_of_balance: [1, 10]
    count_of_swaps: [1, 5]
    swap_back_to_native: true
    retry_count: 3
  faucet:
    enabled: true
    # Get your API key here: https://2captcha.com/enterpage
    twocaptcha_key:
    retry_captcha: 3
    retry_count: 3
  liquidity:
    count_of_transactions: [1, 4]
    enabled: true
    percentage_of_balance: [1, 5]
    retry_count: 3
    remove: true
    slippage: 10 
  checkin:
    enabled: false
    retry_count: 3
    pause_between_attempts: [10, 30]

